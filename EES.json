{
    $jason:
    {
        head:
        {
            actions:
            {
                displayBanner:
                {
                    type: "$util.banner"
                    options:
                    {
                        title: "Clicked"
                        description: "Link {{$jason.url}} clicked!"
                    }
                }
            }
        }
        body:
        {
            background:
            {
                type: "html"
                url: "http://www.kafedra-ees.ru/"
                action:
                {
                    trigger: "displayBanner"
                }
            }
        }
    }
}
